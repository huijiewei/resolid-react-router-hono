{
  "name": "@resolid/react-router-hono",
  "version": "2.3.1",
  "description": "React Router Hono Server",
  "keywords": [
    "resolid",
    "react-router",
    "hono",
    "vercel",
    "netlify"
  ],
  "homepage": "https://www.resolid.tech",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huijiewei/resolid-react-router-hono.git"
  },
  "license": "MIT",
  "author": "Huijie Wei",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/types.mjs"
    },
    "./dev": {
      "types": "./dist/dev.d.mts",
      "import": "./dist/dev.mjs"
    },
    "./node-server": {
      "types": "./dist/node-server.d.mts",
      "import": "./dist/node-server.mjs"
    },
    "./node-preset": {
      "types": "./dist/node-preset.d.mts",
      "import": "./dist/node-preset.mjs"
    },
    "./netlify-server": {
      "types": "./dist/netlify-server.d.mts",
      "import": "./dist/netlify-server.mjs"
    },
    "./netlify-preset": {
      "types": "./dist/netlify-preset.d.mts",
      "import": "./dist/netlify-preset.mjs"
    },
    "./vercel-server": {
      "types": "./dist/vercel-server.d.mts",
      "import": "./dist/vercel-server.mjs"
    },
    "./vercel-preset": {
      "types": "./dist/vercel-preset.d.mts",
      "import": "./dist/vercel-preset.mjs"
    }
  },
  "main": "./dist/types.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@vercel/nft": "^1.1.1",
    "esbuild": "^0.27.0",
    "fast-glob": "^3.3.3",
    "hono": "^4.10.7",
    "minimatch": "^10.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@resolid/config": "^3.10.3",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/node": "^22.19.1",
    "eslint": "^9.39.1",
    "lefthook": "^2.0.5",
    "prettier": "^3.7.3",
    "tsdown": "^0.16.8",
    "type-fest": "^5.3.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "@netlify/types": "^2.2.0",
    "@react-router/dev": "^7.10.0",
    "react-router": "^7.10.0",
    "vite": "^7.2.6"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": "^20.19.0 || ^22.13.0 || >=24"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ],
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@^1",
      "array.prototype.findlast": "npm:@nolyfill/array.prototype.findlast@^1",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@^1",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@^1",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@^1",
      "hasown": "npm:@nolyfill/hasown@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "isarray": "npm:@nolyfill/isarray@^1",
      "object.assign": "npm:@nolyfill/object.assign@^1",
      "object.entries": "npm:@nolyfill/object.entries@^1",
      "object.fromentries": "npm:@nolyfill/object.fromentries@^1",
      "object.values": "npm:@nolyfill/object.values@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "safe-regex-test": "npm:@nolyfill/safe-regex-test@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "string.prototype.includes": "npm:@nolyfill/string.prototype.includes@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
      "string.prototype.repeat": "npm:@nolyfill/string.prototype.repeat@^1"
    }
  }
}
