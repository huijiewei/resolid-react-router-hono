{
  "name": "@resolid/react-router-hono",
  "version": "2.1.0",
  "type": "module",
  "sideEffects": false,
  "description": "React Router Hono Server",
  "author": "Huijie Wei",
  "license": "MIT",
  "keywords": [
    "resolid",
    "react-router",
    "hono",
    "vercel",
    "netlify"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "tsdown",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.js"
    },
    "./node-server": {
      "types": "./dist/node-server.d.ts",
      "import": "./dist/node-server.js"
    },
    "./node-preset": {
      "types": "./dist/node-preset.d.ts",
      "import": "./dist/node-preset.js"
    },
    "./netlify-server": {
      "types": "./dist/netlify-server.d.ts",
      "import": "./dist/netlify-server.js"
    },
    "./netlify-preset": {
      "types": "./dist/netlify-preset.d.ts",
      "import": "./dist/netlify-preset.js"
    },
    "./vercel-server": {
      "types": "./dist/vercel-server.d.ts",
      "import": "./dist/vercel-server.js"
    },
    "./vercel-preset": {
      "types": "./dist/vercel-preset.d.ts",
      "import": "./dist/vercel-preset.js"
    }
  },
  "dependencies": {
    "@hono/node-server": "^1.19.5",
    "@vercel/nft": "^0.30.2",
    "esbuild": "^0.25.10",
    "hono": "^4.9.12",
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@netlify/types": "^2.1.0",
    "@resolid/config": "^3.9.0",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/node": "^22.18.10",
    "eslint": "^9.37.0",
    "lefthook": "^1.13.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsdown": "^0.15.7",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@netlify/types": "^2.1.0",
    "@react-router/dev": "^7.9.4",
    "react-router": "^7.9.4",
    "vite": "^7.1.9"
  },
  "peerDependenciesMeta": {
    "@netlify/types": {
      "optional": true
    }
  },
  "engines": {
    "node": "^20.19.0 || ^22.13.0 || >=24"
  },
  "packageManager": "pnpm@10.18.2",
  "homepage": "https://www.resolid.tech",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huijiewei/resolid-react-router-hono.git"
  },
  "pnpm": {
    "overrides": {
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "isarray": "npm:@nolyfill/isarray@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "array-includes": "npm:@nolyfill/array-includes@^1",
      "array.prototype.findlast": "npm:@nolyfill/array.prototype.findlast@^1",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@^1",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@^1",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@^1",
      "hasown": "npm:@nolyfill/hasown@^1",
      "object.assign": "npm:@nolyfill/object.assign@^1",
      "object.entries": "npm:@nolyfill/object.entries@^1",
      "object.fromentries": "npm:@nolyfill/object.fromentries@^1",
      "object.values": "npm:@nolyfill/object.values@^1",
      "safe-regex-test": "npm:@nolyfill/safe-regex-test@^1",
      "string.prototype.includes": "npm:@nolyfill/string.prototype.includes@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
      "string.prototype.repeat": "npm:@nolyfill/string.prototype.repeat@^1"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ]
  }
}
